.ico_eye
  +p-el(before, 60%, 60%)
    absolute(left 20% top 20%)
    background: $color
    border-top-right-radius: 80%
    border-bottom-left-radius: @border-top-right-radius
    transform: rotate(-45deg)
  +p-el(after, 20%, 20%)
    absolute(left 35% top 35%)
    border: 4px solid $bgcolor
    border-radius: 100%
    margin-left: -2px
    margin-top: -2px